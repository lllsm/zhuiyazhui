/* pages/photoshare/photoshare.wxss */
.container {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-flow: column nowrap;
  overflow: hidden;
  background-color: #fff;
}
.container-content {
  margin: 40rpx;
  padding: 40rpx;
  
}

.container-content-img {
  margin: 50rpx 0;
  display: flex;
  justify-content: center;
}

.img-msg {
  font-size: 12px;
  color: #999;
}


.msg {
  margin-top: 50rpx;
  text-align: center;
}

.icon_complete {
  font-size: 80rpx;
  color: #2c2c22;
}

.bottom {
  margin: 0 auto;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  bottom: 40px;
}


