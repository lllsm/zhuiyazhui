      <view class="margin-right-15">
        <view class="h6 txt-bold nowrap margin-bottom-5" style="text-align: right;">短视频去水印</view>
        <view class="h7 text-2 nowrap type-text-1 " style="text-align: right;">N款平台官方直解</view>
