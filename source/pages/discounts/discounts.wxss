  justify-content: center;
  align-items: center;
}

.cs {
  text-align: center;
  padding-bottom: 80rpx;
}

.cs .foodName {
  font-size: 20px;
  color: #fec456;
  margin-top: 50rpx;
  width: auto;
  height: 50px;
  font-weight: 600;
}

.randomBtn {
  display: flex;
  height: 80rpx;
  padding: 0;
  width: 300rpx;
  margin: 0 auto;
  align-items: center;
  justify-content: center;
  margin-top: 30rpx;
  background: #dfdfdf;
  color: #333;
  font-size: 30rpx;
  border-radius: 40rpx;
}

.t {
  padding-right: 10rpx;
