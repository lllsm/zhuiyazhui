/* pages/discounts/discounts.wxss */
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.hb {
  width: 70rpx;
  height: 70rpx;
}

.im {
  width: 100%;
  height: 100%;
}

.tab-top {
  display: flex;
  align-items: center;
  padding: 8rpx;
  border: 6rpx solid rgba(0,0,0,0);
  border-bottom: 0;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  font-weight: bold;
  padding-bottom: 16rpx;
  box-sizing: border-box;
}
.box-bb {
  flex: 1;
  box-sizing: border-box;
  position: relative;
  border-top: 12px solid #f6f6f6;
  
}

.innerbox-o {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  background-color: #f6f6f6;
  border-top: 0;
}

.act {
  position: relative;
  /* background-color: #f6f6f6; */
  z-index: 100;
}

.bottom-bb {
  height: constant(safe-area-inset-bottom);
  height: env(safe-area-inset-bottom);
}

.title {
  text-align: center;
  font-weight: bold;
  font-size: 32rpx;
  color: #666;
  padding-top: 40rpx;
}

.hb-flex {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.hb-flex .hb {
  width: 320rpx;
  height: 200rpx;
  background-color: #fff;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow:2rpx 2rpx 5rpx #999;
}

.hb-t {
  font-weight: bold;
}

.hb-icon {
  width: 130rpx;
  height: 130rpx;
  flex-shrink: 0;
}

.flex2 .hb {
  height: 150rpx;
}

.flex2 .hb .hb-icon {
  width: 110rpx;
  height: 110rpx;
}

.xh {
  padding-left: 60rpx;
  padding-right: 60rpx;
  background-color: #fff;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 20rpx;
  margin-top: 30rpx;
  line-height: 46rpx;
  letter-spacing: 5rpx;
}

.xiao {
  width: 50rpx;
  height: 50rpx;
}

.more {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
}

.more-t {
  margin-left: 10rpx;
  color: #fec456;
  font-weight: bold;
}

.canvas-box {
  padding-top: 30rpx;
